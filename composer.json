{
    "name": "rebib/phonenumber",
    "description": "Japanese phone number",
    "type": "package",
    "keywords": [        
        "hyphen",
        "Japanese phone number"
    ],
    "license": "mit",
    "authors": [
        {
            "name": "Zhao",
            "email": "zhao2050@gmail.com"
        }
    ],    
    "require": {
        "php": "^7.2",
        "phpoffice/phpspreadsheet": "^1.9",
        "symfony/yaml": "^4.3"
    },
    "bin": [
        "phpunit"
    ],
    "autoload": {
        "psr-4": {
            "Rebib\\": "src/Rebib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rebib\\Test\\": "tests/Rebib/Test"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
